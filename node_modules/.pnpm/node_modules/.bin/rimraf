#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/colchones/univercidad/semestre_7/desarrollo-d-a-e/backend-suscridash/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/node_modules:/home/colchones/univercidad/semestre_7/desarrollo-d-a-e/backend-suscridash/node_modules/.pnpm/rimraf@2.7.1/node_modules:/home/colchones/univercidad/semestre_7/desarrollo-d-a-e/backend-suscridash/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/colchones/univercidad/semestre_7/desarrollo-d-a-e/backend-suscridash/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/node_modules:/home/colchones/univercidad/semestre_7/desarrollo-d-a-e/backend-suscridash/node_modules/.pnpm/rimraf@2.7.1/node_modules:/home/colchones/univercidad/semestre_7/desarrollo-d-a-e/backend-suscridash/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
